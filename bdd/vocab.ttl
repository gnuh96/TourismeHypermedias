@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tagr: <https://restauranttag.univ-paris8.fr/onto/tagr#> .
@prefix ma: <http://www.w3.org/ns/ma-ont#>.

<https://restauranttag.univ-paris8.fr/onto/tagr#> a owl:Ontology ;
    dcterms:title "Restaurant tag" .

tagr:Restaurants a rdfs:Class ;
    rdfs:label "Restaurants" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

tagr:Tags a rdfs:Class ;
    rdfs:label "Tags" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .